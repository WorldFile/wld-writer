{
  "name": "wld-writer",
  "version": "0.1.0",
  "description": "Write World Files (e.g., .gfw, .jgw, .j2w, .pgw, .tfw, and .wld)",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "scripts": {
    "f": "npm run format",
    "format": "npx prettier --arrow-parens=avoid --print-width=160 --trailing-comma=none --write *.js *.ts",
    "test": "npm run test:js && npm run test:ts",
    "test:js": "node ./test.js",
    "test:ts": "npx ts-node ./test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WorldFile/wld-writer.git"
  },
  "keywords": [
    "wld",
    "world-file",
    "gfw",
    "jgw",
    "j2w",
    "pgw",
    "tfw",
    "gis",
    "imagery",
    "satellite"
  ],
  "author": "Daniel J. Dufour",
  "license": "CCO-1.0",
  "bugs": {
    "url": "https://github.com/WorldFile/wld-writer/issues"
  },
  "homepage": "https://github.com/WorldFile/wld-writer#readme",
  "devDependencies": {
    "flug": "^2.3.1"
  }
}
